{
    "name": "boomcms/boom-installer",
    "description": "BoomCMS installer",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Taylor",
            "email": "rob@uxblondon.com"
        }
    ],
    "require": {
        "Laravel/Framework": "5.1.*",
	"boomcms/boom-core": ">=3.0"
    },
    "require-dev": {
        "boomcms/boom-core": "dev-master"
    },
    "autoload": {
        "psr-4": {"BoomCMS\\": "src/BoomCMS/"}
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6",
        "Mockery/Mockery": "^0.9.4"
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "dist"
    }
}
